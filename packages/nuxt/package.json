{
  "name": "@vue3-apollo/nuxt",
  "version": "1.0.0",
  "description": "Nuxt module for Apollo Client integration with Vue 3",
  "repository": "https://github.com/guendev/vue3-apollo",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run prepack && npm publish && git push --follow-tags",
    "typecheck": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.3",
    "ohash": "^2.0.11",
    "@vue3-apollo/core": "workspace:^",
    "defu": "^6.1.4"
  },
  "peerDependencies": {
    "@apollo/client": "^4.0.0",
    "graphql": "^16.0.0",
    "graphql-ws": "^6.0.0",
    "vue": "^3.5.0"
  },
  "peerDependenciesMeta": {
    "graphql-ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@apollo/client": "^4.0.7",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@types/node": "latest",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.6",
    "nuxt": "^4.1.3"
  }
}
