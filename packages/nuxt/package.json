{
  "name": "@vue3-apollo/nuxt",
  "version": "1.0.0",
  "description": "Nuxt module for Apollo Client integration with Vue 3",
  "keywords": [
    "vue",
    "vue3",
    "nuxt",
    "apollo",
    "graphql",
    "module"
  ],
  "license": "MIT",
  "author": "Guen <heloo@guen.dev>",
  "homepage": "https://github.com/guendev/vue3-apollo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guendev/vue3-apollo.git"
  },
  "bugs": {
    "url": "https://github.com/guendev/vue3-apollo/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build prepare && nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "typecheck": "pnpm run prepack && vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.3",
    "ohash": "^2.0.11",
    "@vue3-apollo/core": "workspace:^",
    "defu": "^6.1.4"
  },
  "peerDependencies": {
    "@apollo/client": "^4.0.0",
    "graphql": "^16.0.0",
    "graphql-ws": "^6.0.0",
    "vue": "^3.5.0",
    "vue-tsc": "^3.1.1"
  },
  "peerDependenciesMeta": {
    "graphql-ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@apollo/client": "^4.0.7",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@types/node": "latest",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.6",
    "nuxt": "^4.1.3"
  },
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
