{
  "name": "vue3-apollo",
  "type": "module",
  "version": "0.0.1",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:types",
    "build:js": "tsup --config tsup.config.ts",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist"
  },
  "peerDependencies": {
    "@apollo/client": "^4.0.0",
    "graphql": "^16.0.0",
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0"
  },
  "devDependencies": {
    "@vue/tsconfig": "^0.8.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
