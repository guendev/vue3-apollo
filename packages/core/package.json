{
  "name": "@vue3-apollo/core",
  "type": "module",
  "version": "1.2.0",
  "packageManager": "pnpm@10.18.2",
  "description": "Composable Apollo Client utilities for Vue 3.",
  "author": "Guen <heloo@guen.dev>",
  "license": "MIT",
  "homepage": "https://github.com/guendev/vue3-apollo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guendev/vue3-apollo.git"
  },
  "bugs": {
    "url": "https://github.com/guendev/vue3-apollo/issues"
  },
  "keywords": [
    "vue",
    "vue3",
    "apollo",
    "graphql",
    "composables",
    "client"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "tsup --config tsup.config.ts --watch",
    "build": "tsup --config tsup.config.ts",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@apollo/client": "^4.0.0",
    "graphql": "^16.0.0",
    "vue": "^3.5.0"
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0"
  },
  "devDependencies": {
    "@vue/tsconfig": "^0.8.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  }
}
